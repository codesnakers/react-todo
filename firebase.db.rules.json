{
  "rules": {
    "users": {
      "$uid": {
        ".read": "$uid === auth.uid",
        ".write": "$uid === auth.uid"
      }
    },
    "lists": {
		"$uid": {
			".read": "$uid === auth.uid",
			".write": "$uid === auth.uid"
		}
    },
	"items": {
		"$uid": {
			".read": "$uid === auth.uid",
			".write": "$uid === auth.uid"
		}
    }
  }
}